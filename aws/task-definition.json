{
  "family": "wingstop-poc",
  "networkMode": "awsvpc",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "1024",
  "memory": "2048",
  "executionRoleArn": "<+pipeline.variables.executionRoleArn>",
  "taskRoleArn": "<+pipeline.variables.taskRoleArn>",
  "containerDefinitions": [
    {
      "name": "wingstop-poc",
      "image": "677276094537.dkr.ecr.us-east-1.amazonaws.com/<+pipeline.stages.Install_Dependencies.spec.execution.steps.push_ecr.artifact_push_ecr.stepArtifacts.publishedImageArtifacts[0].imageName>@<+pipeline.stages.Install_Dependencies.spec.execution.steps.push_ecr.artifact_push_ecr.stepArtifacts.publishedImageArtifacts[0].digest>",
      "cpu": 1024,
      "memory": 2048,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8080,
          "hostPort": 8080,
          "protocol": "tcp"
        }
      ]
    }
  ]
}